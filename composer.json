{
    "name": "fincallorca/datetimebundle",
    "type": "symfony-bundle",
    "description": "Extends symfony projects with a simplified server-database-client timezone management and additional DateTime methods.",
    "keywords": [
        "datetime",
        "timezones",
        "symfony",
        "php",
        "database",
        "utc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falko Matthies",
            "email": "falko.m@web.de"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "doctrine/dbal": "^2.5",
        "prokki/ext-datetime": "^0.0.0",
        "symfony/yaml": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/config": "^4.3",
        "symfony/http-kernel": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Fincallorca\\DateTimeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fincallorca\\DateTimeBundle\\Test\\": "tests/"
        }
    }
}
